<script>
  // Función para cambiar entre horarios
  function initScheduleSelector() {
    const buttons = document.querySelectorAll('.schedule-btn');
    const contents = document.querySelectorAll('.schedule-content');
    const noSelection = document.getElementById('no-selection');

    buttons.forEach(button => {
      button.addEventListener('click', () => {
        const scheduleId = button.getAttribute('data-schedule');
        
        // Ocultar el mensaje de "no hay selección"
        if (noSelection) {
          noSelection.classList.add('hidden');
        }
        
        // Remover clase active de todos los botones
        buttons.forEach(btn => {
          btn.classList.remove('active', 'bg-orange-500', 'bg-green-500', 'bg-purple-500', 'bg-teal-500', 'text-white', 'dark:bg-orange-600', 'dark:bg-green-600', 'dark:bg-purple-600', 'dark:bg-teal-600');
          btn.classList.add('bg-gray-200', 'text-gray-700', 'dark:bg-gray-700', 'dark:text-gray-300');
        });
        
        // Agregar clase active al botón clickeado con su color específico
        button.classList.remove('bg-gray-200', 'text-gray-700', 'dark:bg-gray-700', 'dark:text-gray-300');
        button.classList.add('active', 'text-white');
        
        // Aplicar color específico según la línea
        if (scheduleId === 'palmira-colonia') {
          button.classList.add('bg-orange-500', 'dark:bg-orange-600');
        } else if (scheduleId === 'carmelo-mercedes') {
          button.classList.add('bg-green-500', 'dark:bg-green-600');
        } else if (scheduleId === 'palmira-carmelo') {
          button.classList.add('bg-purple-500', 'dark:bg-purple-600');
        } else if (scheduleId === 'palmira-agraciada') {
          button.classList.add('bg-teal-500', 'dark:bg-teal-600');
        }
        
        // Ocultar todos los contenidos
        contents.forEach(content => {
          content.classList.add('hidden');
        });
        
        // Mostrar el contenido seleccionado
        const selectedContent = document.querySelector(`[data-schedule-content="${scheduleId}"]`);
        if (selectedContent) {
          selectedContent.classList.remove('hidden');
          // Scroll suave hacia el contenido con un pequeño delay para la animación
          setTimeout(() => {
            selectedContent.scrollIntoView({ behavior: 'smooth', block: 'start' });
          }, 100);
        }
      });
    });
  }

  // Función para manejar selección de horarios individuales
  function initScheduleRowSelector() {
    const scheduleRows = document.querySelectorAll('.schedule-row');

    scheduleRows.forEach(row => {
      row.addEventListener('click', function() {
        // Remover selección de todas las filas en la misma tabla
        const table = this.closest('table');
        const rowsInTable = table.querySelectorAll('.schedule-row');
        
        rowsInTable.forEach(r => {
          r.classList.remove('selected-schedule', 'bg-orange-100', 'dark:bg-orange-900/50', 'ring-2', 'ring-orange-500');
        });

        // Agregar selección a esta fila
        this.classList.add('selected-schedule', 'bg-orange-100', 'dark:bg-orange-900/50', 'ring-2', 'ring-orange-500');
        
        // Scroll suave hacia la fila seleccionada
        this.scrollIntoView({ behavior: 'smooth', block: 'center' });
      });

      // Doble clic para deseleccionar
      row.addEventListener('dblclick', function() {
        this.classList.remove('selected-schedule', 'bg-orange-100', 'dark:bg-orange-900/50', 'ring-2', 'ring-orange-500');
      });
    });
  }

  // Inicializar cuando el DOM esté listo
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', () => {
      initScheduleSelector();
      initScheduleRowSelector();
    });
  } else {
    initScheduleSelector();
    initScheduleRowSelector();
  }
</script>

